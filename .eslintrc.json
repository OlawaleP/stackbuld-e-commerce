  {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "next/core-web-vitals",
      "next",
      "plugin:jest/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "jest"
    ],
      "settings": {
    "react": {
      "version": "detect"
    }
  },
    "rules": {
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off",
    }
  }